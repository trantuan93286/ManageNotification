    <link href="~/Content/css/skins/_all-skins.min.css" rel="stylesheet" />
    <link href="~/Content/plugins/bower_components/morris.js/morris.css" rel="stylesheet" />
    <link href="~/Content/plugins/bower_components/bootstrap-datepicker/dist/css/bootstrap-datepicker.min.css" rel="stylesheet" />
   
    <link href="~/Content/plugins/timepicker/bootstrap-timepicker.css" rel="stylesheet" />
    <link href="~/Content/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css" rel="stylesheet" />
    <link href="~/Content/css/alt/AdminLTE-select2.min.css" rel="stylesheet" />
    <link href="~/Content/css/alt/AdminLTE-without-plugins.min.css" rel="stylesheet" /> 
    <link href="~/Content/plugins/iCheck/square/blue.css" rel="stylesheet" />
