    <!-- Morris.js charts -->
    <script src="~/Content/plugins/bower_components/raphael/raphael.min.js"></script>
    
    <script src="~/Content/plugins/bower_components/morris.js/morris.min.js"></script>
    <!-- Sparkline -->
    <script src="~/Content/plugins/bower_components/jquery-sparkline/dist/jquery.sparkline.min.js"></script>
